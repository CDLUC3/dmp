<% if user_signed_in? && !current_user.org.is_other? %>
  <div class="branding-name">
    <%= current_user.org.name %>
  </div>
<% end %>

<div class="branding-logo">
  <%= render partial: "layouts/dmptool/logo" %>
</div>

<% if user_signed_in? && !current_user.org.is_other? %>
  <div class="branding-links">
    <ul>
      <%= render partial: 'layouts/dmptool/org_links' %>
    </ul>
  </div>
<% end %>